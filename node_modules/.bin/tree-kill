#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/fox/Desktop/personal/portfolio2025-BE/node_modules/tree-kill/node_modules:/Users/fox/Desktop/personal/portfolio2025-BE/node_modules:/Users/fox/Desktop/personal/node_modules:/Users/fox/Desktop/node_modules:/Users/fox/node_modules:/Users/node_modules:/node_modules:/Users/fox/Desktop/personal/portfolio2025-BE/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/fox/Desktop/personal/portfolio2025-BE/node_modules/tree-kill/node_modules:/Users/fox/Desktop/personal/portfolio2025-BE/node_modules:/Users/fox/Desktop/personal/node_modules:/Users/fox/Desktop/node_modules:/Users/fox/node_modules:/Users/node_modules:/node_modules:/Users/fox/Desktop/personal/portfolio2025-BE/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tree-kill/cli.js" "$@"
else
  exec node  "$basedir/../tree-kill/cli.js" "$@"
fi
