{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3BucketList",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::cdk-hnb659fds-assets-508261694268-us-east-1"
      ]
    },
    {
      "Sid": "S3ObjectRW",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::cdk-hnb659fds-assets-508261694268-us-east-1/uploads/*",
        "arn:aws:s3:::cdk-hnb659fds-assets-508261694268-us-east-1/resume/*"
      ]
    }
  ]
}
